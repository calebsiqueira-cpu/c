#include <stdio.h>
int main() {
    float idade;
    printf("Qual sua idade: ");
    scanf("%f", &idade);
    
    if (idade >= 18){
        printf("Você é maior de idade, e pode tirar carteira de motorista. ");
    }
    else {
        printf("Você é menor de idade, e não pode dirigir. ");
    }
    return 0;
}
